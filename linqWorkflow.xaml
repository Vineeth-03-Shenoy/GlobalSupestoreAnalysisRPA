    <Assign sap2010:Annotation.AnnotationText="Count the total number of 'First Class' Ship mode which come under 'Consumer' Segment and have Order Priority of 'Critical'" DisplayName="LINQ 1" sap:VirtualizedContainerService.HintSize="479,157" sap2010:WorkflowViewState.IdRef="Assign_1">
